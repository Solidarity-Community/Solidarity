{
	"version": "0.2.0",
	"configurations": [
		{
			"name": ".NET Docker Attach",
			"type": "coreclr",
			"justMyCode": false,
			"request": "attach",
			"processId": "${command:pickRemoteProcess}",
			"pipeTransport": {
				"pipeProgram": "docker",
				"pipeArgs": [
					"exec",
					"-i",
					"solidarity_server"
				],
				"debuggerPath": "/vsdbg/vsdbg",
				"pipeCwd": "${workspaceRoot}/packages/Server",
				"quoteArgs": false
			},
			"sourceFileMap": {
				"/Solidarity": "${workspaceRoot}/packages/server/"
			}
		}
	]
}